<Page>
    <Resources>
        <StyleSheet>
            <Style name="cornerRadiusStyle">
                <Setter property="layer.masksToBounds" value="YES" />
                <Setter property="layer.cornerRadius" value="{{dimen:window_corner_radius}}" />
            </Style>
            
        </StyleSheet>
    </Resources>
    
    <Layout>
        <UIView id="containerView" >
            
            <UIImageView id="gameImageView" style="cornerRadiusStyle" mdoutlet="YES" bounds="{{dimen:user_head_middle_size}},{{dimen:user_head_middle_size}}">
                <Constraints>
                    <Constraint attribute="edges" target="containerView"/>
                </Constraints>
            </UIImageView>
            
        </UIView>
    </Layout>
</Page>